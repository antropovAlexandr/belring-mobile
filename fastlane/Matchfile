git_url("git@github.com:stereodenis/belring-signing.git")

storage_mode("git")
username("stereodenis@gmail.com") # Your Apple Developer Portal username
git_basic_authorization(ENV['GIT_BASIC_AUTHORIZATION'])

type("adhoc") # The default type, can be: appstore, adhoc, enterprise or development

generate_apple_certs(true)
force_for_new_devices(true)

app_identifier("com.belringmobile")

# For all available options run `fastlane match --help`
# Remove the # in the beginning of the line to enable the other options

# The docs are available on https://docs.fastlane.tools/actions/match
